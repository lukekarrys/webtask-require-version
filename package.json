{
  "name": "webtask-require-version",
  "description": "Override require so you can test your webtask.io scripts.",
  "version": "1.0.1",
  "author": "Luke Karrys",
  "bugs": {
    "url": "https://github.com/lukekarrys/webtask-require-version/issues"
  },
  "dependencies": {
    "resolve": "^1.1.7",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-tape-runner": "^2.0.1",
    "git-validate": "^2.1.4",
    "standard": "^8.0.0-beta.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "files": [
    "lib/*"
  ],
  "homepage": "https://github.com/lukekarrys/webtask-require-version",
  "keywords": [
    "require",
    "semver",
    "version",
    "webtask",
    "webtask.io"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "pre-commit": [
    "lint",
    "test",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lukekarrys/webtask-require-version.git"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "lint": "standard",
    "prepublish": "npm run compile",
    "test": "babel-tape-runner test/*.js | tap-spec",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "**/lib/**"
    ]
  }
}
